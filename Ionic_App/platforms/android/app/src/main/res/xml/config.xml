<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.starter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BackgroundMode">
        <param name="android-package" value="de.appplant.cordova.plugin.background.BackgroundMode" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <allow-navigation href="http://localhost:8080/*" />
    <feature name="IonicWebView">
        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" onload="true" value="io.ionic.keyboard.IonicKeyboard" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>Liv2Eat</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="resources/android/icon/logo_transparent.png" />
    <icon density="mdpi" src="resources/android/icon/logo_transparent.png" />
    <icon density="hdpi" src="resources/android/icon/logo_transparent.png" />
    <icon density="xhdpi" src="resources/android/icon/logo_transparent.png" />
    <icon density="xxhdpi" src="resources/android/icon/logo_transparent.png" />
    <icon density="xxxhdpi" src="resources/android/icon/logo_transparent.png" />
    <splash density="land-ldpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="land-mdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="land-hdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="land-xhdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="land-xxhdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="land-xxxhdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-ldpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-mdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-hdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-xhdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-xxhdpi" src="resources/android/splash/logo_transparent.png" />
    <splash density="port-xxxhdpi" src="resources/android/splash/logo_transparent.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="KeepRunning" value="true" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="webView" value="com.ionicframework.cordova.webview.IonicWebViewEngine" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
